/* (dot) .QFrame  fix #141, #126, #123 */
.QFrame {
  border-radius: 0px;
  border: 0px solid #696968;
  background-color: #696969;
  /* No frame */
  /* HLine */
  /* HLine */
}

.QFrame[frameShape=&quot;0&quot;] {
  border-radius: 1.9px;
  border: 1px solid #696968;
}

.QFrame[frameShape=&quot;4&quot;] {
  max-height: 1px;
  border: none;
  background-color: #696968;
}

.QFrame[frameShape=&quot;5&quot;] {
  max-width: 1px;
  border: none;
